(core.define @asm.load (core.lambda (address)
    (core.list (interpres/string/concat @"iload " address))))

(core.define @asm.loadl (core.lambda (integer)
    (core.list (interpres/string/concat @"sipush " (core.integer-to-string integer)))))

(core.define @asm.loadc (core.lambda (character)
    (asm.loadl (interpres/character/ord character))))

(core.define @asm.loads (core.lambda (string)
    (core.list (interpres/string/concat @"ldc \"" string @"\""))))

