(interpres/define @+ (interpres/lambda (left, right)
    (interpres/list left right @("iadd"))))

(interpres/define @- (interpres/lambda (left, right)
    (interpres/list left right @("isub"))))

(interpres/define @* (interpres/lambda (left, right)
    (interpres/list left right @("imul"))))

(interpres/define @/ (interpres/lambda (left, right)
    (interpres/list left right @("idiv"))))

(interpres/define @% (interpres/lambda (left, right)
    (interpres/list left right @("irem"))))

