(interpres/define @+ (interpres/lambda (a b)
    (interpres/list a b @"CALL add")))

(interpres/define @- (interpres/lambda (a b)
    (interpres/list a b @"CALL sub")))

(interpres/define @* (interpres/lambda (a b)
    (interpres/list a b @"CALL mult")))

(interpres/define @/ (interpres/lambda (a b)
    (interpres/list a b @"CALL div")))

(interpres/define @% (interpres/lambda (a b)
    (interpres/list a b @"CALL mod")))

