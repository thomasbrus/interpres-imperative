(core.define @read-int (core.lambda (@variable-symbol)
    (core.list
      (interpres/string/concat @"LOADA " ~(interpres/symbol/concat variable-symbol @: @address))
      @"CALL getint")))

(core.define @read-char (core.lambda (variable-symbol)
    (core.list
      (interpres/string/concat @"LOADA " ~(interpres/symbol/concat variable-symbol @: @address))
      @"CALL get"
      @"CALL geteol")))

